#ifndef HASHTABLE_H_
#define HASHTABLE_H_

class nlist<T> {
  struct nlist* next;
  char* name;
  void (*defn)(int, ...);
};

#define HASHSIZE 101
//static struct nlist* hashtab[HASHSIZE];

unsigned hash(char*);
nlist* lookup(struct nlist**, char*);

char* strdup(char*);
nlist* install(struct nlist**, char*, void (*)(int, ...));

#endif